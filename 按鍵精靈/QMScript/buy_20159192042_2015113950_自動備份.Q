Function startgame
	While True
		FindPic 0,0,1000,650,"C:\ranger\TASK336.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Goto startgame1
		End If
		
	Wend
	Rem startgame1
	While True
		FindPic 0,0,1000,650,"C:\ranger\next.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Goto startgame2
		End If
		
	Wend
	Rem startgame2
	While True
		FindPic 0,0,1000,650,"C:\ranger\start.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Goto startgame3
		End If
		
	Wend
	Rem startgame3
End Function
Function game
	While True
		FindPic 0,0,1000,650,"C:\ranger\auto80y.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY+80
			LeftClick 1
			Delay 800
			LeftClick 1
			Delay 7000
			FindPic 0,0,1000,650,"C:\ranger\ice.bmp",0.6,intX,intY
			If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX+80,intY
			LeftClick 1
			End If
			Goto game1
		End If
		
	Wend
	Rem game1
End Function
Function endgame

	While True
		FindPic 0,0,1000,650,"C:\ranger\end01.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			TracePrint intX
			Delay 3000
			MoveTo intX,intY-100
			LeftClick 1
			Goto endgame1
		End If
		Delay 2000
	Wend
	Rem endgame1
	

		
	While True
		//獲得寶物或升級
		FindPic 0,0,1000,650,"C:\ranger\end04.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
		End If
		FindPic 0,0,1000,650,"C:\ranger\endUp.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
		End If
		
		FindPic 0,0,1000,650,"C:\ranger\end02.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Goto endgame2
		End If
	Wend
	Rem endgame2
	
	While True
		FindPic 0,0,1000,650,"C:\ranger\end03.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Goto endgame3
		End If

		FindPic 0,0,1000,650,"C:\ranger\end06.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Goto endgame3
		End If
	Wend
	
	Rem endgame3
End Function

