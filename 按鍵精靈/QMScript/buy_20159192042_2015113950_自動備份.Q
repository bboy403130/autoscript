Call main

Sub main
	While true
		Call startgame
		Call game
		Delay 45000
		//Delay 20000
		Call endgame
		Delay 1140000
	Wend	
End Sub
Sub friend
Rem friend_start
	While true
		TracePrint 1
		
		FindPic 0,0,1000,650,"C:\ranger\friend01.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Delay 100
			MoveTo intX,intY
			LeftClick 1
			Delay 500
		Else 
			
			Call friendMove
			Goto friend_start
		End If
		While true
			TracePrint 2
	    	FindPic 0,0,1000,650,"C:\ranger\friend02.bmp",0.6,intX,intY
			If intX > 0 And intY > 0 Then 
				Delay 100
				MoveTo intX,intY
				LeftClick 1
				Goto friend_ok
			End If
		Wend
		While true
		Rem friend_ok
			TracePrint 3
			FindPic 0,0,1000,650,"C:\ranger\friend03.bmp",0.6,intX,intY
			If intX > 0 And intY > 0 Then 
				Delay 100
				MoveTo intX,intY
				LeftClick 1
				Goto friend_ok1
			End If
		Wend
		Rem friend_ok1
		Delay 1000
	Wend
End Sub

