End Function

Function buyMoney
	FindPic 0,0,1600,1000,"C:\firstSword\1D.bmp",1,intX,intY
	If intX > 0 And intY > 0 Then 
		TracePrint "好便宜阿"
		Delay 300
		MoveTo intX,intY
		LeftClick 1
		FindPic 0,0,1600,1000,"C:\firstSword\buyIcon.bmp",0.4,intX,intY
		If intX > 0 And intY > 0 Then 
		Call Plugin.Pic.PrintScreen(0,0,1600,1000,"C:\firstSword\"+CStr(Hour(timeindex))+"_"+CStr(Minute(timeindex))+"_"+CStr(Second(timeindex))+".bmp")
		MoveTo intX, intY
		LeftClick 1
		End If

		
	End If
End Function

